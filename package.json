{
  "name": "ngx-mask-ionic",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "ng build ngx-mask-ionic-lib",
    "lib:build-watch": "ng build ngx-mask-ionic-lib --watch",
    "lib:copy-readme": "node_modules/.bin/copyfiles ./README.md ./dist/ngx-mask-ionic-lib",
    "lib:publish": "npm run lib:build && npm run lib:copy-readme && cd dist/ngx-mask-ionic-lib && npm publish",
    "lint-fix": "node_modules/.bin/tslint --fix --project tsconfig.json",
    "format-ts": "node_modules/.bin/prettier --write --list-different \"src/**/*.ts\"",
    "format-html": "node_modules/.bin/glob-run html-beautify -r \"src/**/*.html\"",
    "format-css": "node_modules/.bin/glob-run css-beautify -r \"src/**/*.scss\"",
    "lib:format-ts": "node_modules/.bin/prettier --write --list-different \"projects/**/*.ts\"",
    "lib:format-html": "node_modules/.bin/glob-run html-beautify -r \"projects/**/*.html\"",
    "lib:format-css": "node_modules/.bin/glob-run css-beautify -r \"projects/**/*.scss\"",
    "lint-format": "npm run lint-fix && npm run format-ts && npm run format-html && npm run format-css && npm run lib:format-ts && npm run lib:format-html && npm run lib:format-css",
    "check-for-tslint-prettier-conflicts": "node_modules/.bin/tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.9",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "glob-run": "^0.1.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.8.8",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.1.1"
  }
}
